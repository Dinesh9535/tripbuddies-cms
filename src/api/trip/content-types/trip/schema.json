{
  "kind": "collectionType",
  "collectionName": "trips",
  "info": {
    "singularName": "trip",
    "pluralName": "trips",
    "displayName": "Trips"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "blocks"
    },
    "tripimage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "trip_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::trip-categorie.trip-categorie",
      "mappedBy": "trips"
    }
  }
}
